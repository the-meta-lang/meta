[define [crc32 *input]
	[define divisor 3988292384]
	[define crc 4294967295]
	[define i 0]
	[while [!= &1[+ input i] 0]
		[define byte &1[+ input i]]
		[set crc [xor crc byte]]
		[define j 0]
		[while [!= j 8]
			[if [and crc 1]
				[set crc [xor [zfrs crc 1] divisor]]
				[set crc [zfrs crc 1]]]
			[set j [+ j 1]]]
		[set i [+ i 1]]]
	[return [xor crc 4294967295]]]

[define [xor num1 num2]
	[asm "xor edi, esi"]
	[asm "mov eax, edi"]]

[define [zfrs num1 num2]
	[asm "mov eax, esi"]
	[asm "mov cl, al"]
	[asm "shr edi, cl"]
	[asm "mov eax, edi"]]

[define [and num1 num2]
	[asm "and edi, esi"]
	[asm "mov eax, edi"]]

[crc32 "Hello"]